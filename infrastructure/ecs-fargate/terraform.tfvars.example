# terraform.tfvars.example

# AWS region to deploy to
region = "us-east-1"

# Application name
app_name = "pokemon"

# Port the container listens on
container_port = 5000

# Full container image URI (ECR or Docker Hub)
# Example: "123456789012.dkr.ecr.us-east-1.amazonaws.com/pokemon:latest"
container_image = "your-ecr-or-dockerhub-image-uri"
